class Lap
{
	String name;
	String brand;
	int cost;

	Lap(String name,String brand,int cost)
	{
		this.name=name;
		this.brand=brand;
		this.cost=cost;
	}

	public static void main(String[] args)
	{
		Lap x=new Lap("Lenovo","Thinkpad",55000);
		System.out.println(x.name);
		System.out.println(x.brand);
		System.out.println(x.cost);


	}

}