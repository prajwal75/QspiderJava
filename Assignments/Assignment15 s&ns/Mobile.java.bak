class Mobile
{
	static String mobile="Samsung";
	int cost;
	String model;
	public static void main(String[] args)
	{
		Mobile x1=new Mobile();
		x1.cost=45000;
		x1.model="Galaxy y1";
		System.out.println(mobile);
		System.out.println(x1.model);
		System.out.println(x1.cost);
		
		
		Mobile x2=new Mobile();
		x2.cost=55000;
		x2.model="Galaxy y2";
		System.out.println(mobile);
		System.out.println(x2.model);
		System.out.println(x2.cost);
		
	}

}